<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             xmlns:viewModels="clr-namespace:MauiMvvmApp.ViewModels" 
             x:DataType="viewModels:SplashScreenViewModel"
             x:Class="MauiMvvmApp.Views.SplashScreenView">
    <ContentView.Behaviors>
        <toolkit:EventToCommandBehavior EventName="Loaded" Command="{Binding TestCommand}" />
    </ContentView.Behaviors>
    <Grid>
        <Image Source="dotnet_bot.png" />
    </Grid>
</ContentView>
